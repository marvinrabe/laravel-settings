{
  "name": "marvinrabe/laravel-settings",
  "description": "Store key-value settings in your Laravel application.",
  "keywords": [
    "php",
    "laravel",
    "settings"
  ],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/marvinrabe/laravel-settings/issues",
    "source": "https://github.com/marvinrabe/laravel-settings"
  },
  "authors": [
    {
      "name": "Marvin Rabe",
      "email": "marvin.rabe@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": ">=5.5.0",
    "illuminate/database": ">=5.5.0"
  },
  "require-dev": {
    "orchestra/testbench": "^3.5",
    "phpunit/phpunit": "~7.4"
  },
  "autoload": {
    "psr-4": {
      "MarvinRabe\\LaravelSettings\\": "src/"
    },
    "files": [
      "src/Helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MarvinRabe\\\\LaravelSettings\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MarvinRabe\\LaravelSettings\\Providers\\ServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
